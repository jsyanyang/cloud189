  "files": [
    "dist",
    "compiled"
  ],
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "@vercel/ncc": "0.34.0",
    "dotenv": "16.0.3",
    "father": "4.1.0"
  },
  "dependencies": {
    "@hudiemon/utils": "0.0.3",
    "@hudiemon/webhook": "2.0.1",
    "@types/node": "^18.15.13",
    "await-to-js": "^3.0.0",
    "axios": "1.1.3",
    "bignumber.js": "^9.1.0",
    "nodemon": "^2.0.22",
    "ts-node": "^10.9.1"
  }
